import 'dart:io';

void main() {
  print("Ви прокинулися. Що будете робити?");
  print("q - Піти на навчання");
  print("e - Не йти на навчання");
  
  var value = stdin.readLineSync() ?? "";

  if (value == "q") {
    print("Ви прийшли на навчання і получили хороші оцінки.");
  } else if (value == "e") {
    print("Ви не пішли на навчання і получили н.");
  } else {
    print("Невірний вибір.");
  }
}
